package camion;

public class Moving {
	public static void main(String[] args) {
	int cartonTotal = 34;
	int moving = 0;
	
	while (cartonTotal > 0) {
		if (cartonTotal >= 9) {
			moving = 9;
			System.out.println("Le camion vient faire un trajet avec " + moving);
			cartonTotal -= moving;
			moving = 0;
		} else {
			System.out.println("Le camion vient faire un trajet avec " + cartonTotal);
		}
	}
}
}
